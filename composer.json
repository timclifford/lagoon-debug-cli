{
  "name": "amazeeio/lagoon-debug-cli",
  "description": "Cli debug tool prototype",
  "require": {
    "php": "^8.0",
    "ext-intl": "*",
    "ext-json": "*",
    "doctrine/cache": "^1.10",
    "google/apiclient": "^2.0",
    "guzzlehttp/guzzle": "^7.0",
    "haydenpierce/class-finder": "^0.4.2",
    "io-developer/php-whois": "^4.0",
    "jeremykendall/php-domain-parser": "^5.7",
    "kevinrob/guzzle-cache-middleware": "^3.3",
    "monolog/monolog": "^2.1",
    "nesbot/carbon": "^2.38",
    "omines/oauth2-gitlab": "^3.3",
    "php-di/php-di": "^6.2",
    "itsgoingd/clockwork": "^5.0",
    "spatie/ssl-certificate": "^1.19",
    "symfony/console": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "optimize-autoloader": true,
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
